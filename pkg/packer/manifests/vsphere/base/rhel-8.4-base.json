{
  "boot_wait": "10s",
  "convert_to_template": "false",
  "cpu": "4",
  "cpu_cores": "1",
  "disk_controller_type": "pvscsi",
  "disk_thin_provisioned": "true",
  "disk_type_id": "0",
  "firmware": "bios",
  "format": "",
  "headless": "true",
  "insecure_connection": "false",
  "memory": "8192",
  "network": "",
  "network_card": "vmxnet3",
  "skip_compaction": "false",
  "ssh_timeout": "60m",
  "vmx_version": "15",

  "boot_command_prefix": "<up><tab> inst.text inst.ks=hd:sr1:",
  "boot_command_suffix": "/linux/{{user `distro_name`}}/http/8/ks.cfg<enter><wait>",
  "boot_media_path": "",
  "build_name": "rhel-8.4",
  "distro_arch": "amd64",
  "distro_name": "rhel",
  "distro_version": "8",
  "floppy_dirs": "./linux/{{user `distro_name`}}/http/",
  "kickstart_files": "./linux/{{user `distro_name`}}/http/",
  "authorized_keys_file": "./linux/authorized_keys",
  "guest_os_type": "rhel8-64",
  "iso_checksum": "{{env `ISO_SHA256_CHECKSUM`}}",
  "iso_checksum_type": "sha256",
  "iso_url": "{{env `ISO_URL`}}",
  "os_display_name": "RHEL 8",
  "redhat_epel_rpm": "https://dl.fedoraproject.org/pub/epel/epel-release-latest-8.noarch.rpm",
  "shutdown_command": "/sbin/halt -h -p",
  "vsphere_guest_os_type": "rhel8_64Guest"
}
